{
  "compilerOptions": {
    "target": "ESNext",
    "module": "commonjs",
    "outDir": "./server",
    "rootDir": "./server_src",
    "esModuleInterop": true,
    "strict": true,
    "types": [],
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve"
  },
  "include": [
    "server_src",
    "**.ts"
, "module.exports = {   env: {     customKey: 'my-value',   }, }"  ],
  "exclude": [
    "./frontend",
    "./server"
  ]
}
